{
  "name": "deep-research-monorepo",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=web",
    "dev:api": "turbo run dev --filter=backend",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "prettier": "^3.4.2",
    "turbo": "^2.5.8",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": "22.x",
    "npm": ">=10.0.0"
  },
  "packageManager": "npm@10.9.2"
}
